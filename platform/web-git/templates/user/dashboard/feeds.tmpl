{{range .Feeds}}
	<div class = "time_line">
		<div class = "time_line_entry clearfix">
			<div class = "time_line_start">
				<div class = "time_line_icon">
					<i class = "mega-octicon octicon-{{ActionIcon .GetOpType}}"></i>
				</div>
				<div class = "time_line_time">{{TimeSince .GetCreate $.i18n.Lang}}</div>
			</div>
			<div class = "time_line_label">
				{{$IssueImportanceState := $.IssueImportanceState}}
				<p>
					<img src="{{.ActAvatar}}" />
					<a href="{{AppSubUrl}}/{{.GetActUserName}}" class = "btn-link">{{.ShortActUserName}}</a>
					{{if eq .GetOpType 1}}
						<span>创建了项目</span>
						<a href="{{AppSubUrl}}{{.GetRepoLink}}" class = "btn-link">{{.ShortRepoPath}}</a>
					{{else if eq .GetOpType 2}}
						{{$.i18n.Tr "action.rename_repo" .GetContent .GetRepoLink .ShortRepoPath | Str2html}}
					{{else if eq .GetOpType 5}}
						{{ $branchLink := .GetBranch | EscapePound}}
						<span>推送了</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/src/{{$branchLink}}" class = "btn-link">{{$branchLink}}</a>
						<span>分支的代码到</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
					{{else if eq .GetOpType 6}}
						{{ $index := index .GetIssueInfos 0}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span>创建了任务</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/issues/{{$index}}" class = "btn-link">{{.GetIssueTitle}}</a>
					{{else if eq .GetOpType 7}}
						{{ $index := index .GetIssueInfos 0}}
						{{$.i18n.Tr "action.create_pull_request" .GetRepoLink $index .ShortRepoPath | Str2html}}
					{{else if eq .GetOpType 8}}
						{{$.i18n.Tr "action.transfer_repo" .GetContent .GetRepoLink .ShortRepoPath | Str2html}}
					{{else if eq .GetOpType 9}}
						{{$.i18n.Tr "action.push_tag" .GetRepoLink .GetBranch .ShortRepoPath | Str2html}}
					{{else if eq .GetOpType 10}}
						{{ $index := index .GetIssueInfos 0}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span>评论了任务</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/issues/{{$index}}" class = "btn-link">{{.GetIssueTitle}}</a>
					{{else if eq .GetOpType 11}}
						{{ $index := index .GetIssueInfos 0}}
						{{$.i18n.Tr "action.merge_pull_request" .GetRepoLink $index .ShortRepoPath | Str2html}}
					{{else if eq .GetOpType 12}}
						{{ $index := index .GetIssueInfos 0}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span>中把任务</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/issues/{{$index}}" class = "btn-link">{{.GetIssueTitle}}</a>
						<span>设置为<span class = "text-primary">已解决</span></span>
					{{else if eq .GetOpType 13}}
						{{ $index := index .GetIssueInfos 0}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span>中把任务</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/issues/{{$index}}" class = "btn-link">{{.GetIssueTitle}}</a>
						<span>设置为<span class = "text-danger">未解决</span></span>
					{{else if eq .GetOpType 14}}
						{{ $index := index .GetIssueInfos 0}}
						{{$.i18n.Tr "action.close_pull_request" .GetRepoLink $index .ShortRepoPath | Str2html}}
					{{else if eq .GetOpType 15}}
						{{ $index := index .GetIssueInfos 0}}
						{{$.i18n.Tr "action.reopen_pull_request" .GetRepoLink $index .ShortRepoPath | Str2html}}
					{{else if eq .GetOpType 16}}
						{{ $branchLink := .GetBranch | EscapePound}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span>创建了分支</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/src/{{.GetBranch}}" class = "btn-link">{{.GetBranch}}</a>
					{{else if eq .GetOpType 17}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span>删除了分支</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/src/{{.GetBranch}}" class = "btn-link">{{.GetBranch}}</a>
					{{else if eq .GetOpType 18}}
						{{$.i18n.Tr "action.delete_tag" .GetRepoLink .GetBranch .ShortRepoPath | Str2html}}
					{{else if eq .GetOpType 19}}
						{{$.i18n.Tr "action.fork_repo" .GetRepoLink .ShortRepoPath | Str2html}}
					{{else if eq .GetOpType 20}}
						{{ $index := index .GetIssueInfos 0}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span>中把任务</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/issues/{{$index}}" class = "btn-link">{{.GetIssueTitle}}</a>
						<span>设置为<span class = "text-success">已验收</span></span>
					{{else if eq .GetOpType 21}}
						{{ $index := index .GetIssueInfos 0}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span>中把任务</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/issues/{{$index}}" class = "btn-link">{{.GetIssueTitle}}</a>
						{{if eq .AssigneeId 0}}
							取消了指派人
						{{else}}
							<span class = "mar-rgt">指派给
								<a href = "{{.AssigneeLink}}" class = "btn-link">
									<img class = "img-circle img-xs" src="{{.AssigneeAvatar}}">
									<span class = "ml-5">{{.AssigneeName}}</span>
								</a>
							</span>
						{{end}}
					{{else if eq .GetOpType 22}}
						{{ $index := index .GetIssueInfos 0}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span>中把任务</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/issues/{{$index}}" class = "btn-link">{{.GetIssueTitle}}</a>
						<span>设置为{{index $IssueImportanceState .Importance}}</span>
					{{else if eq .GetOpType 23}}
						{{ $index := index .GetIssueInfos 0}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span>中把任务</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/issues/{{$index}}" class = "btn-link">{{.GetIssueTitle}}</a>
						{{if eq .AddOrDel 0}}
							<span class = "mar-rgt">清除了所有标签
						{{else}}
							<span class = "mar-rgt">{{if eq .AddOrDel 1}}添加新标签{{else}}删除了标签{{end}}
									<span class="label" style="color: {{.LabelInfo.ForegroundColor}}; background-color: {{.LabelInfo.Color}}">
										{{.LabelInfo.Name | Sanitize}}
									</span>
							</span>
						{{end}}
					{{else if eq .GetOpType 24}}
								{{ $index := index .GetIssueInfos 0}}
								<span>在项目</span>
								<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
								<span>中把任务</span>
								<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/issues/{{$index}}" class = "btn-link">{{.GetIssueTitle}}</a>
								{{if eq .Milestone 0}}
								<span class = "mar-rgt">取消了选中的里程碑</span>
								{{else}}
									<span class = "mar-rgt">归属到里程碑</span>
									<a href="{{$.RepoLink}}/{{.ShortRepoPath}}/issues?milestone={{.MilestoneInfo.ID}}" class = "btn-link mar-rgt">{{.MilestoneInfo.Name | Sanitize}}</a>中
								{{end}}
					{{else if eq .GetOpType 25}}
						{{ $index := index .GetIssueInfos 0}}
						<span>在项目</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}" class = "btn-link">{{.ShortRepoPath}}</a>
						<span><b class = "text-danger">删除</b>了任务</span>
						<a href = "{{AppSubUrl}}/{{.ShortRepoPath}}/issues/{{$index}}" class = "btn-link">{{.GetIssueTitle}}</a>
					{{end}}
				</p>
				{{if eq .GetOpType 5}}
					<ul>
						{{ $push := ActionContent2Commits .}}
						{{ $repoLink := .GetRepoLink}}
						{{if $push.Commits}}
						{{range $push.Commits}}
						<li><a class="commit-id" href="{{$repoLink}}/commit/{{.Sha1}}">{{ShortSha .Sha1}}</a> <span>{{.Message}}</span></li>
						{{end}}
						{{end}}
						{{if and (gt $push.Len 1) $push.CompareURL}}<li><a href="{{AppSubUrl}}/{{$push.CompareURL}}">{{$.i18n.Tr "action.compare_commits" $push.Len}}<span class = "panel_more"><i>>></i></span></a></li>{{end}}
					</ul>
				{{else if eq .GetOpType 6}}
				{{else if eq .GetOpType 7}}
					<ul>
						<li>
							{{index .GetIssueInfos 1}}
						</li>
					</ul>
				{{else if eq .GetOpType 10}}
					<ul>
						<li>
							{{$Content := index .GetIssueInfos 1}}
							<div class = "time_line_label_content">{{$Content | Safe}}</div>
						</li>
					</ul>
				{{else if eq .GetOpType 11}}
					<ul>
						<li>
							{{index .GetIssueInfos 1}}
						</li>
					</ul>
				{{else if (or (or (eq .GetOpType 12) (eq .GetOpType 13)) (or (eq .GetOpType 14) (eq .GetOpType 15)))}}

				{{else if eq .GetOpType 16}}


				{{end}}
			</div>
		</div>
	</div>
{{end}}


